<p id="notice"><%= notice %></p>

<h1>Taught Bies</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Instructor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @taught_bies.each do |taught_by| %>
      <tr>
        <td><%= taught_by.course_id %></td>
        <td><%= taught_by.instructor_id %></td>
        <td><%= link_to 'Show', taught_by %></td>
        <td><%= link_to 'Edit', edit_taught_by_path(taught_by) %></td>
        <td><%= link_to 'Destroy', taught_by, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Taught By', new_taught_by_path %>
